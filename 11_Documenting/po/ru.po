#: ../src/guess.c:108
#, c-format
msgid "Usage: ./guess [OPTIONS]\n"
msgstr "Использование: ./guess [ОПЦИИ]\n"

#: ../src/guess.c:109
#, c-format
msgid "    OPTIONS:\n"
msgstr "    ОПЦИИ:\n"

#: ../src/guess.c:110
#, c-format
msgid "        --help  print help message\n"
msgstr "        --help  напечатать информационное сообщение\n"

#: ../src/guess.c:111
#, c-format
msgid "        -r      use Roman numbers\n"
msgstr "        -r      использовать римские числа\n"

#: ../src/guess.c:136
#, c-format
msgid "Choose number between %s and %s\n"
msgstr "Выберите число от %s до %s\n"

#: ../src/guess.c:140
#, c-format
msgid "Choose number between 1 and 100\n"
msgstr "Выберите число от 1 до 100\n"

#: ../src/guess.c:150
#, c-format
msgid "Is your number greater that %s? (yes/no)\n"
msgstr "Ваше число больше %s? (да/нет)\n"

#: ../src/guess.c:155
#, c-format
msgid "Is your number greater that %lu? (yes/no)\n"
msgstr "Ваше число больше %lu? (да/нет)\n"

#: ../src/guess.c:163
msgid "yes"
msgstr "да"

#: ../src/guess.c:167
msgid "no"
msgstr "нет"

#: ../src/guess.c:174
#, c-format
msgid "Your number is %s\n"
msgstr "Ваше число: %s\n"

#: ../src/guess.c:178
#, c-format
msgid "Your number is %lu\n"
msgstr "Ваше число: %lu\n"

#~ msgid "    ОПЦИИ:\n"
#~ msgstr "    ОПЦИИ:\n"
